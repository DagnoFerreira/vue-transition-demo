{
  "env": { "browser": true },
  "extends": "standard",
  "globals": {
    "Vue": false,
    "Sine": false,
    "TweenLite": false
  },
  "rules": {
    "space-before-function-paren": 0,
    "object-property-newline": 0,
    "arrow-parens": 0,
    "new-cap": 0,
    "no-eval": 0,
    "no-new": 0,
    "semi": 0
  }
}
